<h1>Alumni</h1>

<table class="table" >
  <thead>
    <tr>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instructors.each do |instructor| %>
      <tr scope="row">
        <td><%= instructor.first %></td>
        <td><%= instructor.last %></td>
        <td><%= instructor.email %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

