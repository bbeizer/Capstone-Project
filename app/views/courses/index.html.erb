<h1>Courses</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Code</th>
      <th scope="col">Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr scope="row">
        <td><%= course.name %></td>
        <td><%= course.code %></td>
        <td><%= course.desc %></td>
    
      </tr>
    <% end %>
  </tbody>
</table>
